#!/bin/ksh
#*****************************************************************************
#
#  This script is used to generate truncate for user 
#
#  USAGE: gencreate_dev_user username
#
#  By Adi Zohar - Feb 2003
#*****************************************************************************

sqlplus "/ as sysdba" <<EOF
set line 132 pages 0 echo on trimspo on
CREATE USER SENSIS_QUSER_W79878
  IDENTIFIED BY peter_newman
  DEFAULT TABLESPACE SYSAUX
  TEMPORARY TABLESPACE TEMP
  PROFILE DEFAULT
  ACCOUNT UNLOCK;

  GRANT AAM_WORK_QUERY_ROLE TO SENSIS_QUSER_W79878;
  GRANT PD_WORK_QUERY_ROLE TO SENSIS_QUSER_W79878;
  GRANT CTXAPP TO SENSIS_QUSER_W79878;
  GRANT DLV_WORK_QUERY_ROLE TO SENSIS_QUSER_W79878;
  GRANT SO_WORK_QUERY_ROLE TO SENSIS_QUSER_W79878;
  GRANT AUTHWORK_QUERY_ROLE TO SENSIS_QUSER_W79878;
  GRANT PARAM_WORK_QUERY_ROLE TO SENSIS_QUSER_W79878;
  GRANT REF_APPL_QUERY_ROLE TO SENSIS_QUSER_W79878;
  GRANT CONNECTOR_ROLE TO SENSIS_QUSER_W79878;
  GRANT ACMS_WORK_QUERY_ROLE TO SENSIS_QUSER_W79878;
  GRANT REFREAD_QUERY_ROLE TO SENSIS_QUSER_W79878;
  GRANT REFWORK_QUERY_ROLE TO SENSIS_QUSER_W79878;
  GRANT ADS_WORK_QUERY_ROLE TO SENSIS_QUSER_W79878;
  GRANT AFL_WORK_QUERY_ROLE TO SENSIS_QUSER_W79878;
  GRANT SELECT_CATALOG_ROLE TO SENSIS_QUSER_W79878;
grant ADS_WORK_QUERY_ROLE to SENSIS_QUSER_W79878;
grant BIL_WORK_QUERY_ROLE to SENSIS_QUSER_W79878;
grant CGS_WORK_QUERY_ROLE to SENSIS_QUSER_W79878;
grant CYP_WORK_QUERY_ROLE to SENSIS_QUSER_W79878;
grant DM_ALFRESCO_WORK_QUERY_ROLE to SENSIS_QUSER_W79878;
grant DM_WORK_QUERY_ROLE to SENSIS_QUSER_W79878;
grant EPR_WORK_QUERY_ROLE to SENSIS_QUSER_W79878;
grant HM_WORK_QUERY_ROLE to SENSIS_QUSER_W79878;
grant NTF_WORK_QUERY_ROLE to SENSIS_QUSER_W79878;
grant PDLM_WORK_QUERY_ROLE to SENSIS_QUSER_W79878;
grant SP_WORK_QUERY_ROLE to SENSIS_QUSER_W79878;

  ALTER USER SENSIS_QUSER_W79878 DEFAULT ROLE ALL;
  -- 1 Tablespace Quota for SENSIS_QUSER_W79878 
  ALTER USER SENSIS_QUSER_W79878 QUOTA UNLIMITED ON POOL_DATA;
  alter user SENSIS_QUSER_W79878 IDENTIFIED BY peter_newman;

EOF
